<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="in.tejas.gallery">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/galleryicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/galleryicon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="in.tejas.gallery.PhotoActivity"></activity>
        <activity android:name="in.tejas.gallery.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

</manifest>